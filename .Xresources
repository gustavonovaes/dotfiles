!---------------------------!
!          general          !
!---------------------------!

Xcursor.size: 36

! special
*.foreground: #d8fbff
*.background: [80]#010010

! black
*.color0:       #010010
*.color8:       #010010

! red
*.color1:       #ff285b
*.color9:       #ff285b

! green
*.color2:       #04eca3
*.color10:      #04eca3

! yellow
*.color3:       #e1dda0
*.color11:      #e1dda0

! blue
*.color4:       #0f61cd
*.color12:      #0f61cd

! magenta
*.color5:       #ca4675
*.color13:      #ca4675

! cyan
*.color6:       #34fafb
*.color14:      #34fafb

! white
*.color7:       #d8fbff
*.color15:      #d8fbff

!-----------------------!
!        urxvt          !
!-----------------------!

URxvt.background:             [90]#010010
! blurRadios needs URxvt.transparency
! URxvt.blurRadius:             10
URxvt.cursorColor:            #d8fbff
URxvt.depth:                  32
URxvt.fadeColor:              color2
URxvt.fading:                 25
URxvt.font:                   xft:iosevka:size=14
URxvt.hightlightColor:        #3b758c
URxvt.hightlightTextColor:    #43a5d5
URxvt.internalBorder:         30
! URxvt.letterSpace:            -1
URxvt.matcher.button:         1
URxvt.perl-ext-common:        default,matcher,vtwhell
URxvt.pointerColorBackground: color15
URxvt.pointerColorForeground: color14
URxvt.scrollBar:              false
URxvt.urgentOnBell:           false
!URxvt.transparent:           true
URxvt.urlLauncher:            xdg-open
URxvt.urlButton:              1
URxvt.underlineURLs:          True
URxvt.copyCommand:            xclip -i -selection clipboard
URxvt.pasteCommand:           xclip -o -selection clipboard
URxvt.inputMethod:            ibus
URxvt.preeditType:            OverTheSpot
URxvt.iso14755:               false
URxvt.iso14755_52:            false

! Keybindings
URxvt.keysym.Mod1-c: clipboard:copy
URxvt.keysym.Mod1-v: clipboard:paste
URxvt.keysym.Mod4-C-v: clipboard:paste_escaped
URxvt.keysym.M-c: clipboard:copy
URxvt.keysym.M-v: clipboard:paste
URxvt.keysym.M-C-v: clipboard:paste_escaped
URxvt.keysym.M-Escape: keyboard-select:activate
URxvt.keysym.C-Escape: keyboard-select:activate
URxvt.keysym.M-u: url-select:select_next
