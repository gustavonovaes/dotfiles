!---------------------------!
!          general          !
!---------------------------!
Xcursor.size: 24

! special
*.foreground: #d8fbff
*.background: [80]#010010

! black
*.color0:       #010010
*.color8:       #010010

! red
*.color1:       #ff285b
*.color9:       #ff285b

! green
*.color2:       #04eca3
*.color10:      #04eca3

! yellow
*.color3:       #e1dda0
*.color11:      #e1dda0

! blue
*.color4:       #0f61cd
*.color12:      #0f61cd

! magenta
*.color5:       #ca4675
*.color13:      #ca4675

! cyan
*.color6:       #34fafb
*.color14:      #34fafb

! white
*.color7:       #d8fbff
*.color15:      #d8fbff

!-----------------------!
!        urxvt          !
!-----------------------!

URxvt.scrollBar:              false
URxvt.urgentOnBell:           false
URxvt.inputMethod:            ibus
URxvt.preeditType:            OverTheSpot
URxvt.iso14755:               false
URxvt.iso14755_52:            false
URxvt.perl-ext-common:        default,matcher,url-select,selection-to-clipboard

! Style
URxvt.background:             [80]#010010
URxvt.cursorColor:            #d8fbff
URxvt.depth:                  32
URxvt.fadeColor:              #04eca3
URxvt.fading:                 25
URxvt.font:                   xft:iosevka:size=14
URxvt.hightlightColor:        #3b758c
URxvt.hightlightTextColor:    #43a5d5
URxvt.internalBorder:         30
URxvt.pointerColorBackground: color15
URxvt.pointerColorForeground: color14

! Clipboard
URxvt.copyCommand:            xclip -i -selection clipboard
URxvt.pasteCommand:           xclip -o -selection clipboard

! URLs
URxvt.urlButton:              1
URxvt.underlineURLs:          True
URxvt.url-launcher:           /usr/bin/xdg-open
URxvt.matcher.button:         1
URxvt.url-select.launcher:    xdg-open
URxvt.url-select.underline:   true
URxvt.keysym.M-u:             perl:url-select:select_next

!-------------------!
!        rofi       !
!-------------------!
rofi.separator-style:   none
rofi.hide-scrollbar:    true
rofi.font:              Open Sans 18
rofi.color-enabled:     true
!rofi.color-window:      argb:ee2f343f, argb:ee2f343f, argb:ee2f343f
rofi.color-window:      argb:ee010010, argb:ee010010, argb:ee010010
rofi.color-normal:      argb:002f343f, #d8dee8, argb:002f343f, argb:002f343f, #43a5d5
rofi.color-active:      #2f343f, #b48ead, #2f343f, #2f343f, #93e5cc
rofi.color-urgent:      #2f343f, #ebcb8b, #2f343f, #2f343f, #ebcb8b
